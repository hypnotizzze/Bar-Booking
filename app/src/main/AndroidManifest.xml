<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.barbooking">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/bar_booking_icon_round"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/bar_booking_icon_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.BarBooking">
    <activity
      android:name=".BarInfoActivity"
      android:exported="false">
      <meta-data
        android:name="android.app.lib_name"
        android:value="" />
    </activity>
    <activity
      android:name=".BookingActivity"
      android:exported="false">
      <meta-data
        android:name="android.app.lib_name"
        android:value="" />
    </activity>
    <activity
      android:name=".Personal_Account_Activity"
      android:exported="false" />
    <activity
      android:name=".Search_Bar_Activity"
      android:exported="false" />
    <activity
      android:name=".ChangePasswordActivity"
      android:exported="false" />
    <activity
      android:name=".RegisterActivity"
      android:exported="false" />
    <activity
      android:name=".MainActivity"
      android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <meta-data
      android:name="preloaded_fonts"
      android:resource="@array/preloaded_fonts" />
  </application>

</manifest>